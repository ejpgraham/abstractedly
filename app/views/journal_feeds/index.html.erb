<html>
<head></head>
<body class="vsc-initialized">
  <main role="main" class="container">
    <div class="row row-offcanvas row-offcanvas-right">
      <div class="col-12 col-md-9 container">
          <% @journal_feeds.each do |journal_feed| %>
              <%= content_tag(:button, journal_title_and_date(journal_feed), class: "btn btn-primary", type: "button", data: {toggle: "collapse", target: "#" + journal_title_and_date(journal_feed).gsub(" ", "")}) %>

          <% end %>
        </div>
      </div>
    </div>
  </main>
</body>
